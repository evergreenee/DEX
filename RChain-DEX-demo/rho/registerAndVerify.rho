new user, return, print(`rho:io:stdout`) in {
    contract @"register"(username, publicKey) = {
        user!!([*username, *publicKey]) |
        for (@message <- user) {
            print!(message.nth(1))
        }
    }
    |
    contract @"loginVerify"(myPublicKey) = {
        for(@message <- user) {
            match (message.nth(1) == *myPublicKey) {
                true => print!("Login Success")
                false => print!("Login Failure")
            }
        }
    }
}